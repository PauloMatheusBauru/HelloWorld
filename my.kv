<OrdemPedido>
    orientation: "vertical"
    padding: "8dp"
    size_hint: 1, None
    size: "340dp", "150dp"
    pos_hint: {"center_x": .5, "center_y": 0}
    haling: "center"
    elevation: 10
    line_color: 255/255,98/255,0/255,1

    GridLayout:
        size_hint_x: 1
        cols: 2
        MDBoxLayout:
            size_hint_x: 0.6
            orientation: "vertical"
            MDBoxLayout:
                md_bg_color: 1,1,1,1
                size_hint_x: 1
                size_hint_y: 0.3
                MDIcon:
                    icon: "chart-donut"
                    size_hint: 0.2, 1
                    theme_text_color: "Custom"
                    text_color: 255/255,98/255,0/255,1
                MDLabel:
                    size_hint: 1, 1
                    text: root.IDPedido
                    theme_text_color: "Secondary"
                    font_size: "14"
                    halign: "left"
                    bold: True

            MDBoxLayout:
                md_bg_color: 1,1,1,1
                size_hint_x: 1
                size_hint_y: 0.3
                MDIcon:
                    icon: "truck"
                    size_hint: 0.2, 1
                MDLabel:
                    size_hint: 1, 1
                    text: root.Despachado
                    theme_text_color: "Secondary"
                    font_size: "14"
                    bold: True
                    pos_hint: {"center_x": 0.0}

        MDBoxLayout:
            size_hint_x: 0.4
            size_hint_y: None
            orientation: "vertical"
            MDRectangleFlatIconButton:
                text: "Cancelar Compra"
                icon: "language-python"
                font_size: "14"
                line_color: 0, 0, 0, 0
                pos_hint: {"center_x": 0.5}
                theme_text_color: "Custom"
                theme_icon_color: "Custom"
                text_color: 255/255,98/255,0/255,1
                icon_color: 255/255,98/255,0/255,1
                on_release:  root.Cancelamento()

            MDRectangleFlatIconButton:
                text: "Produto com defeito"
                icon: "format-list-bulleted-type"
                theme_text_color: "Custom"
                font_size: "14"
                theme_icon_color: "Custom"
                text_size: "25"
                line_color: 0, 0, 0, 0
                pos_hint: {"center_x": 0.5}
                text_color: 255/255,98/255,0/255,1
                icon_color: 255/255,98/255,0/255,1
                on_release:  root.TrocaProdutoComDefeito()
