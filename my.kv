<ProdutosCarrinho>:
    type_swipe: "hand"
    size_hint_y: None
    size_hint_x: 1
    width: self.width
    swipe_distance: 800
    size: "350dp", "150dp"
    opening_time: 0.0
    MDCardSwipeLayerBox:
        opening_time: 0.0
        padding: "0dp"
        elevation: 10
        radius: [19, ]
        size_hint_y: None
        size_hint_x: None
        md_bg_color: 248/255,17/255,17/255,0.8
        MDLabel:
            size_hint_y: None
            size_hint_x: None
            size: 300, 100
            icon_size: "10"
            md_bg_color: 248/255,17/255,17/255,0.8
            pos_hint: {'center_x': 1, 'center_y': 0.5}
            FloatLayout:
                size_hint_y: None
                size_hint_x: None
                MDFloatingActionButton:
                    size_hint_y: None
                    size_hint_x: None
                    icon_color: 1,1,1,1
                    icon_size: "64sp"
                    md_bg_color: 248/255,17/255,17/255,0.8
                    icon: "trash-can"
                    on_release:
                        root.RemoverCarrinho(root)
                        app.RemoverCarrinho(root)
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
    MDCardSwipeFrontBox:
        opening_time: 0.0
        type_swipe: "auto"
        size_hint_y: 1
        size_hint_x: 1
        width: self.width
        size: "350dp", "150dp"
        swipe_distance: 800
        opening_time: 0.1
        FloatLayout:
            size_hint: 1, 1
            size: root.size
            id: FrenteDeslizar
            MDIconButton:
                icon: "delete"
                id: Delete
                pos_hint: {'center_x': 0.95, 'center_y': 0.9}
                on_release:
                    root.RemoverCarrinho()
                    app.RemoverCarrinho(root)
                    app.AtualizarValores()
            MDLabel:
                size_hint_x: 0.6
                size_hint_y: 1.5
                id: label
                font_size: "18sp"
                text: root.Titulo
                color: .2, .2, .2, .8
                pos_hint: {'center_x': 0.60}

            MDIcon:
                color: 255/255,98/255,0/255,1
                size_hint_x: 1
                icon: "shield-check"
                font_size: 15
                pos_hint: {'center_x': 0.79, 'center_y': 0.4}

            MDLabel:
                size_hint_x: 0.5
                id: label
                text: "Garantia de 90 dias"
                font_size: "10sp"
                color: .2, .2, .2, .8
                pos_hint: {'center_x': 0.57, 'center_y': 0.4}


            MDBoxLayout:
                id: BoxLayoutQuantidade
                md_bg_color: 1,1,1,1
                size_hint_y: 0.1
                size_hint_x: 0.3
                pos_hint: {'center_x': 0.40, 'center_y': 0.1}

                MDIconButton:
                    icon: "plus-circle"
                    on_press:
                        root.Add()
                        app.AtualizarValores()

                    halign: "left"
                    pos_hint: {'center_y': 0.5}

                MDLabel:
                    id: QuantidadeProdutos
                    text: "1"
                    halign: "center"

                MDIconButton:
                    icon: "minus-circle"
                    on_press:
                        root.Remove()
                        app.AtualizarValores()
                    halign: "right"
                    pos_hint: {'center_y': 0.5}

            FitImage:
                elevation: 10
                radius: [19, ]
                size_hint_y: None
                size_hint_x: None
                source: root.Img
                height: 130
                width: 110
                pos_hint: {'center_x': 0.15, 'center_y': 0.5}

            MDLabel:
                id: label
                text: root.Preco
                font_size: "22sp"
                halign: "right"
                bold: True
                color: 255/255,98/255,0/255,1
                pos_hint: {'center_y': 0.1}
